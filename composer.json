{
    "name": "cchhay/laravel-request-search",
    "version": "1.0.0",
    "description": "Laravel request search query and report query.",
    "type": "library",
    "require": {
        "php": ">=8.2"
    },
    "autoload": {
        "psr-4": {
            "Cchhay\\LaravelRequestSearch\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cchhay\\LaravelRequestSearch\\LaravelRequestSearchServiceProvider"
            ],
            "aliases": {
                "RequestSearch": "Cchhay\\LaravelRequestSearch\\Facades\\LaravelRequestSearch",
                "RequestSearchReport": "Cchhay\\LaravelRequestSearch\\Facades\\LaravelRequestSearchReport"
            }
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Chhay Chhem",
            "email": "chhay_ittech@hotmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
